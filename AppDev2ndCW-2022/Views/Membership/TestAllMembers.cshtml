<div class="flex flex-col w-full px-20 mt-16">
    <div class="flex justify-start">
        @foreach (var member in ViewBag.members)
        {
            <div class="flex flex-col">
                <h1>@member.member.MemberFirstName</h1>
                <h1>@member.count</h1>
                <h1>@member.totalAllowedLoans</h1>
            </div>
        }
    </div>
</div>