<div class="flex flex-col justify-start mt-16">
    <h3 class="mb-6 text-sm text-gray-500">Showing all results</h3>
    <div class="grid grid-cols-5 gap-20">

        @foreach (var dvd in ViewBag.allDvd)
        {
            <div class="flex flex-col w-full">
                <div class="h-[10rem] w-[12rem] bg-gray-400">
                </div>
                <div>
                    <h2 class="mt-2 font-medium text-gray-200">@dvd.DvdNumber</h2>
                    <h3 class="text-sm">@dvd.DateReleased</h3>
                    <h3 class="text-sm">@dvd.StandardCharge</h3>
                    <h3 class="text-sm">@dvd.PenaltyCharge</h3>
                </div>
            </div>
        }
